{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
        "api/app.js": {
          "memory": 1024,  
          "maxDuration": 30 
        }
      },
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "/api/app.js"
        }
    ]
  }
  